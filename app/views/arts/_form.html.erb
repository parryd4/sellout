<%= form_for @art do |f| %>
  <%= f.label "Art Title" %>
  <%= f.text_field :title %><br>
  <%= f.label "Asking Price" %>
  <%= f.text_field :price %><br>
  <%= f.label "Description" %>
  <%= f.text_field :description %><br>
  <%= f.hidden_field :user_id, value: session[:user_id] %>
  <%= f.collection_select :category_id, Category.all, :id, :name %>
<%= f.submit %>
<% end %>
